{
  "name": "beastmode-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "ts-node prisma/seed.ts",
    "generate:docker": "docker-compose exec backend npx prisma generate",
    "migrate:docker": "docker-compose exec backend npx prisma migrate dev --name init",
    "studio:docker": "docker-compose exec backend npx prisma studio --hostname 0.0.0.0 --port 5555"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@prisma/client": "^6.6.0",
    "bcrypt": "^5.1.1",
    "beastmode-backend": "file:",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "^6.6.0",
    "prisma-zod-generator": "^0.8.13",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
